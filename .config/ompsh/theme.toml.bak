[palette]
background = "#0f1417"
foreground = "#dfe3e7"
primary = "#90cef4"
secondary = "#b6c9d8"
tertiary = "#cbc1e9"
surface = "#0f1417"
surface_bright = "#353a3d"
outline = "#8b9297"
error = "#ff5945"
shadow = "#000000"

[[blocks]]
alignment = "left"
type = "prompt"
[[blocks.segments]]
style = "plain"
foreground = "p:foreground"
template = "\uF303"
type = "os"
[[blocks.segments]]
foreground = "p:primary"
style = "plain"
template = ' {{ .PWD }} '
type = "path"
[blocks.segments.properties]
style = "agnoster"
[[blocks.segments]]
type = "rust"
foreground = "#FF4F00"
template = 'îž¨ {{ .Full }} '
style = "plain"
[[blocks.segments]]
type = "python"
foreground = "#100e23"
background = "#906cff"
template = ' {{ .Full }} '
style = "plain"
# right prompt
[[blocks]]
alignment = "right"
type = "rprompt"
[[blocks.segments]]
template = ' git: {{ .HEAD }} '
foreground = "p:secondary"
style = "plain"
type = "git"
# time
[[blocks.segments]]
type = "time"
style = "plain"
foreground = "p:primary"
template = ' {{ .CurrentDate | date .Format }} '
